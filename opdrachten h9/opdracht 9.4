void setup(){
  size(400, 400);
  background(255,255,255);
  vierkant(50, 50, 100,100);
}

void draw(){
}
void vierkant(float x, float y, float breedte, float hoogte){
  
  line(x,y,x + breedte, y);
  line(x + breedte, y, x+ breedte, y + hoogte);
  line (x+ breedte, y + hoogte, x, y + hoogte);
  line(x, y + hoogte, x, y);
}
